{
    "version": 2,
    "name": "backend",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/health",
            "dest": "index.js"
        },
        {
            "src": "/api/(.*)",
            "dest": "index.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "index.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}